{
  "name": "ralphschindler/laragis",
  "description": "A geospatial library and set of utilities for Laravel",
  "keywords": ["php", "geo", "gis", "spatial", "geospatial", "laravel"],
  "type": "library",
  "license": "UNLICENSE",
  "authors": [
    {
      "name": "Ralph Schindler"
    }
  ],
  "require": {
    "php": ">=5.5"
  },
  "require-dev": {
    "illuminate/database": "~5.3",
    "illuminate/support": "~5.3",
    "phpunit/phpunit": "~5.5"
  },
  "autoload": {
    "psr-4": {
      "LaraGis\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    },
    "laravel": {
      "providers": [
        "LaraGis\\LaraGisServiceProvider"
      ]
    }
  }
}